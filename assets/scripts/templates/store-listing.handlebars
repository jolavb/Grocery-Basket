
<table class = 'table table-condensed table-bordered'>
   <thead>
     <tr>
       <td><h3>Store</h3></td>
       <td><h3>Location</h3></td>
     </tr>
   </thead>

   <tbody>
     {{#each stores as |store|}}
     <tr class = 'store' data-id= {{store.id}}>
       <td class = >{{store.name}}</td>
       <td>{{store.location}}</td>
     </tr>
     {{/each}}
   </tbody>
 </table>
